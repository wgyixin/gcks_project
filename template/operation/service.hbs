<div class="padding-zero container-fluid">
    <div class="row-fluid span2 " style="padding-top:30px;">
        <div class="row-fluid span12 ">
        <ul class="nav nav-tabs nav-stacked sidebar-nav main-menu">

            {{#if serviceRule}}
                <li class="ajax-link active">
                    {{#link-to 'service'}}服务{{/link-to}}
                </li>
            {{/if}}
            {{#if cnRule}}
                <li class="ajax-link ">
                    {{#link-to 'cn'}}控制服务器{{/link-to}}
                </li>
            {{/if}}
            {{#if rnRule}}
                <li class="ajax-link">
                    {{#link-to 'rn'}}媒体服务器{{/link-to}}
                </li>
            {{/if}}
            <!--<li class="ajax-link">-->
            <!--{{#link-to 'database'}}数据库{{/link-to}}-->
            <!--</li>-->
            <!--<li class="ajax-link">-->
            <!--{{#link-to 'webServer'}}Web Server{{/link-to}}-->
            <!--</li>-->
            {{#if hostsRule}}
                <li class="ajax-link">
                    {{#link-to 'mainframe'}}主机{{/link-to}}
                </li>
            {{/if}}
            {{#if logRule}}
                <li class="ajax-link">
                    {{#link-to 'logs'}}日志{{/link-to}}
                </li>
            {{/if}}
        </ul>
        </div>
    </div>
    <div class="row-fluid span10">

        <div class="box span4">
            <div class="box-header well">
                <h2><i class="icon-th"></i>视频播放服务</h2>
                <div class="box-icon">
                    <a href="#" class="btn btn-setting btn-round"><i class="icon-cog "></i></a>
                    <a href="#" class="btn btn-minimize btn-round"><i class="icon-chevron-up"></i></a>
                    <a href="#" class="btn btn-close btn-round">  <i class="icon-remove"> </i> </a>
                </div>
            </div>
            <div class="box-content">

                    <ul>
                      {{#each video in videoPlayService}}
                          rnList : {{#link-to 'rn'}} {{video.rnList}} {{/link-to}}<br>
                          host : {{#link-to 'mainframe'}} {{video.host}} {{/link-to}}<br>
                          cn : {{#link-to 'cn'}} {{video.cn}} {{/link-to}}<br>
                          {{#each  video.details}}
                              <hr>
                              host:{{host}}<br>
                              RTP_RECIEVER:{{RTP_RECIEVER}}<br>
                              RTP_SENDER:{{RTP_SENDER}}<br>
                              completeTask:{{completeTask}}<br>
                              currentTask:{{currentTask}}<br>
                              record_Read:{{record_Read}}<br>
                              record_Write:{{record_Write}}<br>
                          {{/each}}
                      {{/each}}
                     </ul>
            </div>
        </div>

        <div class="box span4">
            <div class="box-header well">
                <h2><i class="icon-th"></i> 视频录像服务</h2>
                <div class="box-icon">
                    <a href="#" class="btn btn-setting btn-round"><i class="icon-cog "></i></a>
                    <a href="#" class="btn btn-minimize btn-round"><i class="icon-chevron-up"></i></a>
                    <a href="#" class="btn btn-close btn-round">  <i class="icon-remove"> </i> </a>
                </div>
            </div>
            <div class="box-content">
                <ul>
                    {{#each record in recordService}}
                        rnList : {{#link-to 'rn'}} {{record.rnList}} {{/link-to}}<br>
                        host : {{#link-to 'mainframe'}} {{record.host}} {{/link-to}}<br>
                        cn : {{#link-to 'cn'}} {{record.cn}} {{/link-to}}<br>

                        {{#each  record.details}}
                            <hr>
                            host:{{host}}<br>
                            RTP_RECIEVER:{{RTP_RECIEVER}}<br>
                            RTP_SENDER:{{RTP_SENDER}}<br>
                            completeTask:{{completeTask}}<br>
                            currentTask:{{currentTask}}<br>
                            record_Read:{{record_Read}}<br>
                            record_Write:{{record_Write}}<br>
                        {{/each}}
                    {{/each}}
                </ul>

            </div>
        </div>
        <div class="box span4">
            <div class="box-header well">
                <h2><i class="icon-th"></i> HDFS</h2>
                <div class="box-icon">
                    <a href="#" class="btn btn-setting btn-round"><i class="icon-cog "></i></a>
                    <a href="#" class="btn btn-minimize btn-round"><i class="icon-chevron-up"></i></a>
                    <a href="#" class="btn btn-close btn-round">  <i class="icon-remove"> </i> </a>
                </div>
            </div>
            <div class="box-content">
               <p>
                   key : value<br>
                   <a href="http://10.7.100.31:8080/#/login" target="_blank">konw more about HDFS</a>

               </p>
            </div>
        </div>




    </div>
</div>
